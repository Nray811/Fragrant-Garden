@section topCSS{

<style>       
        /* 背景顏色 、 賣完的效果 */
        body{background-color:#e9e9e9; }
        .card{background-color: whitesmoke }
        .navbar{ position: sticky; top: 0; z-index: 200;}
        .soldoutcard {
        position: relative;
        display: flex;
        }
        .empty{
            color:red
        }
        .imgw img {width:100% ; height:auto}
        .soldout {
        width: 180px;
        height: 180px;
        position: absolute;
        background-color: #4c4c4c5c;
        color: #ffffff;
        border-radius: 50%;
        display: flex;
        justify-content: center;
        align-content: center;
        align-items: center;
        font-size: large;
        font-weight: bold;
        text-align: center;
        left: 50%;
        top: 50%;
        transform: translate(-50%, -50%);
        }
        /* 商品選擇列 */
        .Topic {display:flex;  padding:0 ; }
        .Topic li{display:inline; padding:0 2px ; margin: 2px 2px 2px 0px;}
        /* 做成圓形圖片 */
        .bdradius img{ border-radius: 50%; } 
        .card-img-top {
            width:100%;
            height:200px;
            padding:0;
        }
        .tcenter{text-align: center;}
        /* 不要邊框 */
        .bdnone {border:none}
        .container{padding:10px; } 
        a{ text-decoration: none; color:navy}
        .container button{border: none; background: none;}  
        /* 右下角定位 */
        .btt{
        position: fixed;
        bottom: 4%;
        right: 2%;
        background-color: #fff;
        width: 50px;
        height: 50px;
        display: flex;
        justify-content: center;
        align-items: center;
        border-radius: 50%;
        }
        .btt i{
        font-size: 30px;
        color: #8b8b8b;
        }   
    /* product list */
.classification {
    display: flex;
    flex-wrap: wrap;
    position: relative;
    justify-content: center;
    list-style: none;
}

.classification .classify{
    margin: 5px 5px;
    background-color: #AEB6BF;
    color: #ffffff;
    width: 10%;
    height: 30px;
    flex-wrap: wrap;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 10px;

}
.classification .classify:hover{
    background-color: #F7DC6F ;
    cursor: pointer;
}

.classify_list{
    display: flex;
    position: absolute;
    background-color: #fff;
    width: 100%;
    top: 36px;
    left: 0;
    border-top: #cccccc solid 1px;
    border-radius: 10px;
    z-index: 100;

}

ul li .classify_list{ display:none;}

.classify_list .brand{
    width: 10%;
}
.classify_list .brand ul{
    list-style: none;
    font-size: larger;
}
.classify_list .brand ul li{    color: #797979; margin-top: 10px;}
.classify_list .brand ul li:hover{    color: #e4b229;}

.classify_list .content{
    width: 90%;
    display: flex;
    flex-wrap: wrap;
}
.content .content-card{
    display: flex;
    flex-wrap: wrap;
    flex-direction: column;
    width: calc(100% / 4 - 20px);
}
.content .content-card img {width: 100%; }
    /*Footer Css註解*/
    .footer-clean {
    padding:50px 0;
    background-color:#e9e9e9;
    color:#4b4c4d;
    }

    .footer-clean h3 {
    margin-top:0;
    margin-bottom:12px;
    font-weight:bold;
    font-size:16px;
    }

    .footer-clean ul {
    padding:0;
    list-style:none;
    line-height:1.6;
    font-size:14px;
    margin-bottom:0;
    }

    .footer-clean ul a {
    color:inherit;
    text-decoration:none;
    opacity:0.8;
    }

    .footer-clean ul a:hover {
    opacity:1;
    }

    .footer-clean .item.social {
    text-align:right;
    }

    @@media (max-width:767px) {
    .footer-clean .item {
        text-align:center;
        padding-bottom:20px;
    }
    }

    @@media (max-width: 768px) {
    .footer-clean .item.social {
        text-align:center;
    }
    }
    .footer-clean .item.social > a {
    font-size:24px;
    width:40px;
    height:40px;
    line-height:40px;
    display:inline-block;
    text-align:center;
    border-radius:50%;
    border:1px solid #ccc;
    margin-left:10px;
    margin-top:22px;
    color:inherit;
    opacity:0.75;
    }

    .footer-clean .item.social > a:hover {
    opacity:0.9;
    }

    @@media (max-width:991px) {
    .footer-clean .item.social > a {
        margin-top:40px;
    }
    }

    @@media (max-width:767px) {
    .footer-clean .item.social > a {
        margin-top:10px;
    }
    }

    .footer-clean .copyright {
    margin-top:14px;
    margin-bottom:0;
    font-size:13px;
    opacity:0.6;
    }
    
</style>

}


